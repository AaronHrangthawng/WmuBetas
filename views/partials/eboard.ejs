<!-- E-BOARD SECTION -->
<section id="eboard" class="bg-white text-center fade-in-section py-5">
  <div class="container">
    <h2 class="mb-5" style="color: #7851A9;">Executive Board</h2>
    <div class="row g-4 justify-content-center">
      <% members.forEach(member => { %>
        <div class="col-12 col-sm-6 col-md-4 col-lg-3">
          <img src="/images/eboard/<%= member.image %>" alt="<%= member.name %> - <%= member.position %>" class="img-fluid rounded-circle mb-2" style="width: 200px;">
          <h5 class="fw-bold"><%= member.position %></h5>
          <p><%= member.name %></p>
        </div>
      <% }) %>
    </div>
  </div>
</section>
